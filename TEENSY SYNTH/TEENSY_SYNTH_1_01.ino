            if ((ch2_Arpeggiator(5,0) == LOW) || ((ch2_Arpeggiator(5,0) == 
               HIGH) && (ch2_Arpeggiator(7,0) == HIGH) && 
               (ch2_Arpeggiator(8,0) == LOW))) {

