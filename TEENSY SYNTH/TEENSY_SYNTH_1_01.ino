         defaultDutyCycleOn = map(insideCC5, 0, 127, 50, 100);
         dutyCycleOffFactor = map(insideCC1, 0, 127, defaultDutyCycleOff, 200);
         dutyCycleOnFactor = map(insideCC1, 0, 127, defaultDutyCycleOn, 0);
         
