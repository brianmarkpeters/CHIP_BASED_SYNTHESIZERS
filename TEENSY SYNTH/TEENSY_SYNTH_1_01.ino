            else if (insideDecay == LOW) {
               j = 0;
               newestNoteDuration = 0;
               for (j = 0; j < 16; j++) {
                  if ((heldNoteNumberDurationMillis[j] > newestNoteDuration) && 
                     (insideDecay == LOW)) {
                     
